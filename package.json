{
  "name": "quaequasi",
  "version": "1.0.0",
  "description": "webpack plugin to resolve TypeScript files when importing with js file extension in ESM projects",
  "keywords": [
    "ESM",
    "molestias",
    "totam",
    "ullam",
    "quod"
  ],
  "author": "LouisaDeckow",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeraWindler/quaequasi.git"
  },
  "license": "ISC",
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "4.3.2",
    "@softwareventures/prettier-config": "2.0.0",
    "@softwareventures/semantic-release-config": "2.0.0",
    "@softwareventures/tsconfig": "5.1.0",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "prettier": "2.5.1",
    "semantic-release": "17.4.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack": "5.66.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/files"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "publishConfig": {
    "access": "public"
  }
}